{
  "name": "@aura-stack/tsup-config",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Tsup configuration for Aura Stack Auth packages.",
  "scripts": {
    "build": "tsup tsup.config.ts --format cjs,esm --dts --clean",
    "format": "prettier --write . --cache --cache-location .cache/.prettiercache",
    "format:check": "prettier --check . --cache --cache-location .cache/.prettiercache",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "clean:cts": "rm -rf dist/*.cts"
  },
  "keywords": [
    "typescript",
    "tsconfig",
    "configuration",
    "aura-stack"
  ],
  "author": "Aura Stack <aurastackjs@gmail.com> | Hernan Alvarado <hernanvid123@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/tsup.config.d.ts",
      "import": "./dist/tsup.config.js",
      "require": "./dist/tsup.config.cjs"
    }
  },
  "devDependencies": {
    "@aura-stack/tsconfig": "workspace:*"
  },
  "packageManager": "pnpm@10.15.0"
}
