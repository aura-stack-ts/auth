{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "!.turbo/**"]
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": false
    }
  },
  "ui": "tui",
  "cacheDir": ".turbo/cache"
}
